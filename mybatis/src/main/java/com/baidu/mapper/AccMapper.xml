        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baidu.mapper.AccMapper">
    <insert id="save" parameterType="com.baidu.Entity.AccEntity">
        insert into t_acc(username,usersex,age) values(#{username},#{usersex},#{age})
    </insert>
</mapper>

